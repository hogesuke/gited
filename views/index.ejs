<html>
<head>
<link rel='stylesheet' href='/style/gited.css' />
<link rel="stylesheet" href="/icomoon/style.css">
<script type="text/javascript" src="/js/jquery.min.js"></script>
<script type="text/javascript" src="/js/jquery.socialbutton-1.9.1.min.js"></script>
<script type="text/javascript" src="/js/gited.js"></script>
<script type="text/javascript" src="http://www.google.com/jsapi"></script>
<script type="text/javascript">google.load( 'swfobject', '2.2' );</script>
<link rel="shortcut icon" href="/images/favicon.ico">
<title>gited</title>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-50979306-1', 'gited.net');
  ga('send', 'pageview');

</script>
</head>
<body>
<div id="description">
  <div id="main-title">GIT END ROLL</div>
  <div id="catch-copy">Create an end role with commit logs</div>
  <div id="social-container">
    <button class="social-button"><i class="icon-twitter"></i></button>
    <button class="social-button"><i class="icon-facebook"></i></button>
    <button class="social-button"><i class="icon-hatebu"></i></button>
  </div>
</div>
<section id="repositories">
  <% if (userType === 'tryUser') { %>
    <form id="login-form" action="/login/github" method="post">
      <input type="submit" id="login-button" value="GitHub LOGIN"/>
    </form>
  <% } else { %>
    <form id="login-form" action="/logout" method="post">
      <input type="submit" id="logout-button" value="GitHub LOGOUT"/>
    </form>
  <% } %>
  <div id="user-info">
    <% if (userType === 'tryUser') { %>
    <div id="user-type">※現在はお試し用ユーザーを表示しています。</div>
    <% } %>
    <div id="user-name">■ユーザー： <%= name %></div>
  </div>
  <h1>Repositories</h1>
  <ul class="repo-list">
  </ul>
</section>
<div id="screen"></div>
<div id="stop-button" class="inactive">終劇</div>
</body>
</html>
